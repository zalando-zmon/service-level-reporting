{
  "name": "slr",
  "version": "1.0.0",
  "description": "Service Level Reports User Interface",
  "main": "index.html",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cd ./build/default && ../../node_modules/polymer-cli/bin/polymer.js serve --proxy-path '/api/' --proxy-target 'http://localhost:8080/api/'",
    "dev": "./node_modules/polymer-cli/bin/polymer.js serve --proxy-path '/api/' --proxy-target 'http://localhost:8080/api/'",
    "mock": "./node_modules/polymer-cli/bin/polymer.js serve --proxy-path '/api/' --proxy-target 'http://localhost:8000/'",
    "build": "./node_modules/polymer-cli/bin/polymer.js install && ./node_modules/polymer-cli/bin/polymer.js build",
    "test": "./node_modules/polymer-cli/bin/polymer.js test -l chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zalando-zmon/service-level-reporting.git"
  },
  "keywords": [
    "service",
    "level",
    "reports",
    "slr",
    "slo",
    "sli"
  ],
  "author": "Emanuel Lauria",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zalando-zmon/service-level-reporting/issues"
  },
  "homepage": "https://github.com/zalando-zmon/service-level-reporting#readme",
  "dependencies": {
    "bower": "^1.8.4",
    "opentracing-javascript-utils": "^1.0.3",
    "opentracing": "^0.14.1",
    "polymer-cli": "^1.7.0"
  }
}
